{
  "name": "arvo-log-server",
  "version": "0.0.1",
  "description": "Gaze analysis & visualization for tests in ARVO",
  "main": "./src/app.ts",
  "scripts": {
    "build": "tsc",
    "serve": "ts-node-dev --respawn --transpileOnly ./src/app.ts -- -d ../data",
