    "@types/command-line-usage": "^5.0.1",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.1",
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.0.2",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "node-json-db": "^1.0.1",
    "ts-node-dev": "^1.0.0-pre.41",
    "typescript": "^3.5.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "esm": "^3.2.25"
  }
}
